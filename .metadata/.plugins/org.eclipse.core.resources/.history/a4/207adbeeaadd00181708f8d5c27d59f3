
public class MotDict {
	private String mot;
	private String definition;
	
	public String getMot()
	{
		return mot;
	}
	public String getDefinition()
	{
		return definition;
		
	}
	public void setDefinition(String def)
	{
		definition = def;
		
	}
	public void setMot(String mot)
	{
		this.mot = mot;
	}
	
	
	public boolean Synonyme()
	{
		return true;
	}
}
