
public class Champions {
	
		private float health = 100f;
		private float armour = 100f;
		private String name = "";
		
		private static float xp = 0f;
		
		public float getHealth()
		{
			return health;
		}
		public void setHealth(float health)
		{
			this.health = health;
		}
		
		public void Punch()
		{
			//Punch other players
		}
		
		public void CastSpell(float damage)
		{
			//Will cast spell
			health -= damage;
			
		}
}
